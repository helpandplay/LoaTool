<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:Double x:Key="AppWidth">200</sys:Double>
    <sys:Double x:Key="AppHeight">50</sys:Double>
    <sys:Double x:Key="AppOpacity">0.5</sys:Double>
    <CornerRadius x:Key="AppCornerRadius">25</CornerRadius>
    <Thickness x:Key="AppPadding">10</Thickness>
    <sys:Boolean x:Key="AppAlwaysOnTop">True</sys:Boolean>
    <sys:Boolean x:Key="AppAllowsTransparency">True</sys:Boolean>
    <ResizeMode x:Key="AppResizeMode">NoReSize</ResizeMode>
    <SolidColorBrush x:Key="AppBackground" Color="Transparent"/>
    <WindowStartupLocation x:Key="AppWindowStartupLocation">Manual</WindowStartupLocation>
    <WindowStyle x:Key="AppWindowStyle">None</WindowStyle>

    <sys:Double x:Key="IconWidth">30</sys:Double>
    <sys:Double x:Key="IconHeight">30</sys:Double>

    <sys:Double x:Key="TitleIconWidth">16</sys:Double>
    <sys:Double x:Key="TitleIconHeight">16</sys:Double>

    <sys:Double x:Key="ColorPickerWidth">400</sys:Double>
    <sys:Double x:Key="ColorPickerHeight">200</sys:Double>

    <sys:Double x:Key="AuctionWidth">400</sys:Double>
    <sys:Double x:Key="AuctionHeight">200</sys:Double>
</ResourceDictionary>