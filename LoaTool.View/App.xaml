<Application x:Class="LoaTool.View.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LoaTool.View"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/LoaTool.Define;component/Views/Resources/AppStyles.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/LoaTool.Define;component/Views/Resources/AppDefines.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/LoaTool.Define;component/Views/Resources/Icons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
